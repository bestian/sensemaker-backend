{
  "name": "sensemaker-backend",
  "main": "src/index-queue.ts",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": [
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "OPENROUTER_MODEL": "openai/gpt-oss-20b:free",
    "IS_DEVELOPMENT": "true"
  },
  "r2_buckets": [
    {
      "binding": "SENSEMAKER_RESULTS",
      "bucket_name": "sensemaker-results",
      "preview_bucket_name": "sensemaker-results-preview"
    }
  ],
  "queues": {
    "producers": [
      {
        "binding": "SENSEMAKER_QUEUE",
        "queue": "sensemaker-tasks"
      }
    ],
    "consumers": [
      {
        "queue": "sensemaker-tasks",
        "max_batch_size": 1,
        "max_batch_timeout": 30,
        "max_retries": 3,
        "dead_letter_queue": "sensemaker-failed-tasks"
      }
    ]
  }
}
